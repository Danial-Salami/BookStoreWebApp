@model List<Company>

<div class="card shadow border-1 mt-4">
    <div class="card-header ml-0 py-3 bg-dark  ">
        <div class="row ">    
                <div class="col-12 text-center">
                    <h2 class="text-white py-2"> Company List</h2>
                </div>
        </div>
    </div>
    <div class="card-body p-4 ">
        <div class="row pb-3">

            <div class="col-6">
               
            </div>
            <div class="col-6 text-end ">
                <a class="btn btn-success" asp-controller="Company" asp-action="Upsert">
                    <i class="bi bi-plus-circle"></i> Create Company
                </a>
            </div>
        </div>

        <table id="tblData" class="table table-striped table-bordered w-100">
            <thead>
                <tr >
                    <th>
                        Name
                    </th>
                    <th>
                        State
                    </th>
                    <th>
                        City
                    </th>
                  
                    <th>
                        StreetAddress
                    </th>
                    <th>
                        PostalCode
                    </th>
                    <th>
                        PhoneNumber
                    </th>
                  
                    <th>

                    </th>
                </tr>
            </thead>
        </table>
    </div>
</div>
@section Scripts {
    <script src="~/js/company.js"></script>
}
